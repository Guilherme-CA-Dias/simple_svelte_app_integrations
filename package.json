		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/jsonwebtoken": "^9.0.7",
		"concurrently": "^8.2.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@integration-app/react": "^2.1.5",
		"jsonwebtoken": "^9.0.2"
	}
}
